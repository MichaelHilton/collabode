<% helpers.setHtmlTitle(project.getName()) %>
<% helpers.includeCss("editor_ejs.css") %>

<div id="editorpage">

<div id="editortop">
  <div id="docbar">
    <div id="docbarproject"><span><%= project.getName() %></span></div>
  </div>
</div>

<div id="padmain">
  <div id="padeditor">
    <form id="wizardcontainerbox" method="post">
      <h1>Create a Java project</h1>
      <p>Project name: <%= project.getName() %></p>
      
      <div id="wizardbottom">
        <button type="submit">Create project</button>
      </div>
    </form>
  </div>
</div>

</div>
